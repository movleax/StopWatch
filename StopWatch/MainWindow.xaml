<Window x:Class="StopWatch.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StopWatch"
        mc:Ignorable="d"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        Title="MainWindow" Height="505.592" Width="960.526" Closing="Window_Closing">
    <Grid x:Name="Grid">
        <DockPanel Name="DockPanel">
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="_File">
                    <MenuItem Header="_Close" Click="MenuItem_Close_Click"/>
                </MenuItem>
                <MenuItem Header="_Task">
                    <MenuItem Header="_Create Task" Click="MenuItem_CreateTask_Click"/>
                    <MenuItem Header="_View Task List"/>
                </MenuItem>
            </Menu>
            <!--<StackPanel></StackPanel>-->
        </DockPanel>
        
    </Grid>
</Window>
